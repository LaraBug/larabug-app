{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0",
    "calebporzio/sushi": "^2.1",
    "coconutcraig/laravel-postmark": "^2.9",
    "cviebrock/eloquent-sluggable": "^8.0.1",
    "fideloper/proxy": "^4.2",
    "filament/filament": "^v1.12.0",
    "fruitcake/laravel-cors": "^2.0",
    "google/recaptcha": "~1.1",
    "guzzlehttp/guzzle": "^7.3",
    "inertiajs/inertia-laravel": "^v0.4.4",
    "kblais/laravel-uuid": "^1.1",
    "kreait/firebase-php": "^5.19",
    "larabug/larabug": "^2.4",
    "laravel-notification-channels/fcm": "^2.2.1",
    "laravel-notification-channels/webhook": "^2.2",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1.1",
    "laravel/horizon": "^5",
    "laravel/octane": "^1.0",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/socialite": "^5",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.2",
    "predis/predis": "~1.0",
    "reinink/remember-query-strings": "^0.1.0",
    "renatomarinho/laravel-page-speed": "^2.0",
    "spatie/browsershot": "^3.4",
    "spatie/eloquent-sortable": "^3.0",
    "spatie/laravel-cookie-consent": "^2.11",
    "spatie/laravel-honeypot": "^4.0",
    "spatie/laravel-medialibrary": "^8",
    "spatie/laravel-ray": "^1.26",
    "spatie/laravel-sitemap": "^6.0.4",
    "spiral/roadrunner": "^2.0",
    "tedivm/jshrink": "^1.3",
    "tightenco/ziggy": "^1.2",
    "tucker-eric/eloquentfilter": "^2.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.8",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.1.0",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "nunomaduro/larastan": "^0.7.13",
    "pestphp/pest": "^1.20",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpstan/phpstan": "^0.12.99"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Utilities/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "classmap": [
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan horizon:publish"
    ],
    "format": [
      "vendor/bin/php-cs-fixer fix"
    ],
    "phpstan": "vendor/bin/phpstan analyse"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-debugbar",
        "barryvdh/laravel-ide-helper"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
